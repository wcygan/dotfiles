{
    "ignorePatterns": [
        {
            "pattern": "^http://localhost"
        },
        {
            "pattern": "^https://localhost"
        },
        {
            "pattern": "^file://"
        }
    ],
    "replacementPatterns": [
        {
            "pattern": "^/",
            "replacement": "{{BASEURL}}/"
        }
    ],
    "httpHeaders": [
        {
            "urls": [
                "https://github.com"
            ],
            "headers": {
                "User-Agent": "Mozilla/5.0 (compatible; markdown-link-check)"
            }
        }
    ],
    "timeout": "20s",
    "retryOn429": true,
    "retryCount": 3,
    "fallbackRetryDelay": "30s",
    "aliveStatusCodes": [
        200,
        206
    ]
}